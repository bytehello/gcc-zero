type (
	LoginReq {
		Username string `json:"username"`
		Password string `json:"password"`
	}
	LoginReply {
		Code         string `json:"code"`
		Msg          string `json:"msg"`
		Id           int64  `json:"id"`
		Username     string `json:"username"`
		AccessToken  string `json:"accessToken"`
		AccessExpire int64  `json:"accessExpire"`
	}
)

@server (
	group: sys/user
)

service admin-api {
	@handler login
	post /sys/user/login (LoginReq) returns (LoginReply)
}